package com.company;

public class Main {

    public static void main(String[] args) {
	// write your code here
        //as per bodmas
        int a= 5*6-34/2;
        System.out.println("as per bodmas it should print 13");
        System.out.println(a);
        System.out.println("in an equation when precednece is equal then comes associativity ");
